<UserControl x:Class="estacionamentoApp.View.Cadastro.CADValores"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:estacionamentoApp.View.Cadastro"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBox x:Name="txtMinuto" HorizontalAlignment="Left" Height="32" Margin="147,74,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="165"/>
        <TextBox x:Name="txtMeiaHora" HorizontalAlignment="Left" Height="32" Margin="147,113,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="165"/>
