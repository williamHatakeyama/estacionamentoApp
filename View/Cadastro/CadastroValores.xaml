        <Label x:Name="label1" Content="Valor Meia Hora" HorizontalAlignment="Left" Margin="40,107,0,0" VerticalAlignment="Top" Height="37" Width="102"/>
        <Label x:Name="label2" Content="Valor Hora" HorizontalAlignment="Left" Margin="40,147,0,0" VerticalAlignment="Top" Height="36" Width="102"/>
