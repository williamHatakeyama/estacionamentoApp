        <Button x:Name="btnCadastrar" Content="Cadastrar" HorizontalAlignment="Left" Margin="147,259,0,0" VerticalAlignment="Top" Width="120" Background="#FFF0A727" Click="BtnCadastrar_Click"/>
        <TextBox x:Name="textIDV" HorizontalAlignment="Left" Height="32" Margin="147,33,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="165" IsEnabled="False"/>
    </Grid>
</UserControl>
